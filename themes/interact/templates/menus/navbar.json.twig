{
    "results": [ {{ "\n" }}
		{%- for search_result in search_results|slice(0,4) -%}
		{{ '\t\t' }}{"title":"{{- search_result.title -}}", "url":"{{- search_result.url -}}", "text":"{% if search_result.date %}Last Modified: <em>{{ search_result.date|nicetime(false) }}</em>&nbsp;{% endif %}"}{{ not loop.last ? ',' }}{{ "\n" }}
