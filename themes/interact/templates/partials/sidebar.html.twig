{% if config.plugins.random.enabled %}
<div class="sidebar-content">
	<h4>Random Article</h4>
	<a class="uk-button uk-button-secondary" href="{{ base_url_relative }}/random"><i class="uk-icon-retweet"></i> I'm Feeling Lucky!</a>
</div>
            <br />
{% endif %}
<div class="sidebar-content">
	<h4>Some Text Widget</h4>
	<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna.</p>
</div>
            <br />
{% if config.plugins.taxonomylist.enabled %}
<div class="sidebar-content">
    <h4>Popular Keywords</h4>
    {% include 'partials/taxonomylist.html.twig' with {'taxonomy':'tag'} %}
</div>
            <br />
{% endif %}
{% if config.plugins.archives.enabled %}
<div class="sidebar-content">
    <h4>Archives</h4>
	{% include 'partials/archives.html.twig' %}
</div>
            <br />
{% endif %}
{% if config.plugins.feed.enabled %}
<div class="sidebar-content syndicate">
    <h4>Syndicate</h4>
    <a class="uk-button uk-button-secondary" href="{{ feed_url }}.atom"><i class="uk-icon-rss-square"></i> Atom 1.0</a>
    <a class="uk-button uk-button-secondary" href="{{ feed_url }}.rss"><i class="uk-icon-rss-square"></i> RSS</a>
</div>
{% endif %}